schaal = 250
stappen = 20

r = 0
herhaal stappen + 1 {
    x = schaal x cos r
    y = schaal x sin r
    
    pen dikte 3
    naar x y
    naar 0 0
    naar x y
    pen dikte 1
    
    r = r + (PI / stappen)
}
naar 0 0

