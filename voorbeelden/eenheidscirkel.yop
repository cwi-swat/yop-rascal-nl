schaal = 250
stappen = 20

r = 0
herhaal stappen + 1 {
    x = schaal x sin r
    y = schaal x cos r
    
    naar x y
    naar 0 0
    naar x y
    
    r = r + (PI / stappen)
}
naar 0 0

